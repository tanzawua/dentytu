<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5">





    <ImageView
        android:id="@+id/fondo"
        android:layout_width="441dp"
        android:layout_height="910dp"
        android:contentDescription="todo1"
        android:foregroundGravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/fondo45"
        tools:layout_editor_absoluteY="-16dp"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="186dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/miperfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/usuario" />

    <Button
        android:id="@+id/miperfil"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        app:backgroundTint="@null"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="mi perfil"
        android:orientation="horizontal"
        android:textColor="#EFE9E9"
        android:textSize="30sp"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:strokeColor="#029FF3" />

    <Button
        android:id="@+id/button8"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="cambiar de cuenta"
        android:textColor="#EFE9E9"
        android:textSize="30sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/miperfil"
        app:strokeColor="#029FF3" />

    <Button
        android:id="@+id/button9"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="eliminar cuenta"
        android:textColor="#DACCCC"
        android:textSize="30sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        app:strokeColor="#029FF3" />

    <Button
        android:id="@+id/button10"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="Enviar comentarios"
        android:textColor="#ECE5E5"
        android:textSize="30sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:strokeColor="#029FF3" />

    <Button
        android:id="@+id/button11"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="Pantalla acesibilidad"
        android:textColor="#D8CACA"
        android:textSize="28sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10"
        app:strokeColor="#029FF3" />

    <Button
        android:id="@+id/button12"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="Ayuda y soporte técnico"
        android:textColor="#DACBCB"
        android:textSize="28sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/button13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11"
        app:strokeColor="#C4CFD5" />

    <Button
        android:id="@+id/button13"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="Configuración y privacidad"
        android:textColor="#DAC9C9"
        android:textSize="27sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/cerrarsesion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12"
        app:strokeColor="#BECDD5" />

    <Button
        android:id="@+id/cerrarsesion"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="350dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/custon_button"
        android:fontFamily="@font/fuente6"
        android:text="cerrar sesion"
        android:textColor="#D8CCCC"
        android:textSize="30sp"
        app:backgroundTint="@null"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button13"
        app:strokeColor="#029FF3" />

    <ImageButton
        android:id="@+id/retroceder"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:background="@drawable/custon_button"
        android:contentDescription="@string/app_name"
        app:backgroundTint="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable" />


</androidx.constraintlayout.widget.ConstraintLayout>