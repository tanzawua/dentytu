        android:fontFamily="@font/fuente6"
        android:textSize="25dp"
        android:text="recepcion"
        app:backgroundTint="@null"
        android:background="@drawable/custon_button2"
        app:layout_constraintBottom_toTopOf="@+id/button24"
        app:layout_constraintEnd_toStartOf="@+id/button23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button23"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/custon_button2"
        android:fontFamily="@font/fuente6"
        android:textSize="25dp"
        android:text="pagos"

        app:backgroundTint="@null"
        app:layout_constraintBottom_toTopOf="@+id/button25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button21"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button24"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="38dp"
        android:background="@drawable/custon_button2"
        android:fontFamily="@font/fuente6"
        android:text="servicios tecnicos"
        android:textSize="25dp"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button21" />

    <Button
        android:id="@+id/button25"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="38dp"
        app:backgroundTint="@null"
        android:background="@drawable/custon_button2"
        android:fontFamily="@font/fuente6"
        android:textSize="25dp"
        android:text="ayuda y comentarios "

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button24"
        app:layout_constraintTop_toBottomOf="@+id/button23" />


    <ImageButton
        android:id="@+id/retroceder"
        android:layout_width="53dp"
        android:layout_height="32dp"
        android:background="@drawable/custon_button"
        android:contentDescription="@string/app_name"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable" />

</androidx.constraintlayout.widget.ConstraintLayout>